    if(document.querySelector('CV-ATP-845-wrapper') ===  null){
        var utils = optimizely.get('utils');
        utils.waitForElement('#dwfrm_cart_checkoutCart').then(function() {
            var box = document.createElement('div');
              box.className = 'CV-ATP-845-wrapper';
            document.querySelector('.container.clearfix').insertBefore(box, document.querySelector('.container.clearfix > .col-8'));

            var numItems = document.querySelector('h1.checkout-title > span').innerText;
            document.querySelector('h1.checkout-title > span').style.display = 'none';
            var amount = document.querySelector('.cart-widget-value').innerHTML.trim();
            var width = document.querySelector('button[type="submit"][value="Checkout"]').offsetWidth - 10;
            var height = document.querySelector('button[type="submit"][value="Checkout"]').offsetHeight;
            var pTop = document.querySelector('button[type="submit"][value="Checkout"]').style.paddingTop;

            var textElm = document.createElement('div');
            box.appendChild(textElm);
            var firstText = document.createElement('span');
                firstText.innerText = 'Total (' + numItems +')';
                firstText.style.float = 'left';
            textElm.appendChild(firstText);

            var secondText = document.createElement('span');
                secondText.innerText = amount;
                secondText.style.float = 'right';

            textElm.appendChild(secondText);

            var buttonsWrapper = document.createElement('div');

            box.appendChild(buttonsWrapper);

            var payPalbtn = document.createElement('div');
                payPalbtn.className = 'CV-payPalbtn';
                payPalbtn.style.width = width/2.0 + 'px';
                payPalbtn.style.height = height + 'px';
                buttonsWrapper.appendChild(payPalbtn);

                var payPalspan = document.createElement('span');
                payPalspan.innerText = '';
                payPalbtn.appendChild(payPalspan);

            var checkoutbtn = document.createElement('div');
                checkoutbtn.className = 'CV-checkoutbtn';
                checkoutbtn.style.width = width/2.0 + 'px';
                checkoutbtn.style.height = height + 'px';
                buttonsWrapper.appendChild(checkoutbtn);

                var checkoutTxt = document.createElement('span');
                checkoutTxt.innerText = 'Checkout';
                checkoutbtn.appendChild(checkoutTxt);
            //document.querySelector('button[type="submit"][value="Checkout"]').click()

            window.addEventListener('scroll', calcScrollDepth);


            function calcScrollDepth(){
                var headerHeight;
                if(window.innerHeight > window.innerWidth) headerHeight = document.querySelector('h1.checkout-title').offsetHeight;
                else headerHeight = document.querySelector('h1.checkout-title').offsetHeight + document.getElementById('header').offsetHeight;
                var depth = window.scrollY;
                if(depth > headerHeight){
                    if(box.className.indexOf('CV-fixed-header') === -1){
                        box.className += ' CV-fixed-header';
                        document.querySelector('.container.clearfix > .col-8').style.marginTop = '100px';
                    }
                }
                else {
                    box.className = 'CV-ATP-845-wrapper';
                    document.querySelector('.container.clearfix > .col-8').style.marginTop = '0px';
                }
                if(isScrolledIntoView(document.querySelector('button[type="submit"][data-track="paypal checkout"]'))) {
                    //box.style.opacity = 0;
                    box.style.display = 'none';
                    //document.querySelector('.container.clearfix > .col-8').style.marginTop = '0px';
                }
                else {
                    box.style.display = 'block';
                    //box.style.opacity = 1;
                }
            }

            var target = document.querySelector('.header-sticky');

            // create an observer instance
            var observer = new MutationObserver(function(mutation) {

                if(box.className.indexOf('bezierToTop') === -1 && mutation[0].target.className.indexOf('stuck') === -1 && (window.innerHeight > window.innerWidth)){
                    box.classList.remove('bezierReset');
                    box.className += ' bezierToTop';
                }
                else if(box.className.indexOf('bezierReset') === -1 && mutation[0].target.className.indexOf('stuck') > -1 && (window.innerHeight > window.innerWidth)){
                    box.classList.remove('bezierToTop');
                    box.className += ' bezierReset';
                }
            });

            // configuration of the observer:
            var config = { attributes: true, attributeFilter: ["class"]};

            // pass in the target node, as well as the observer options
            observer.observe(target, config);

            function isScrolledIntoView(el) {
                var elemTop = el.getBoundingClientRect().top;
                var elemBottom = el.getBoundingClientRect().bottom;

                var isVisible = (elemTop >= 0) && (elemBottom <= window.innerHeight);
                return isVisible;
            }

            calcScrollDepth();

            window.onresize = function(){
                var newWidth = document.querySelector('button[type="submit"][value="Checkout"]').offsetWidth - 10;
                var newHeight = document.querySelector('button[type="submit"][value="Checkout"]').offsetHeight;

                payPalbtn.style.width = newWidth/2.0 + 'px';
                payPalbtn.style.height = newHeight + 'px';

                checkoutbtn.style.width = newWidth/2.0 + 'px';
                checkoutbtn.style.height = newHeight + 'px';

                calcScrollDepth();
            };

          /*
          	if(window.innerHeight < window.innerWidth){
        			//portrait
              document.querySelector('.CV-ATP-845-wrapper').style.top = 0;
            }*/
              document.querySelector('.CV-payPalbtn').addEventListener('click', function(){
                document.querySelector('button[type="submit"][value="Checkout"]').click();
              });

              document.querySelector('.CV-checkoutbtn').addEventListener('click', function(){
                document.querySelector('button[type="submit"][value="Checkout"]').click();
              });

              document.querySelector('.CV-payPalbtn').addEventListener('touchend', function(){
                document.querySelector('button[type="submit"][data-track="paypal checkout"]').click();
              });

              document.querySelector('.CV-checkoutbtn').addEventListener('touchend', function(){
                document.querySelector('button[type="submit"][value="Checkout"]').click();
              });
        });
    }
}());
