// Create container elemnet and add raw HTML string as innerHTML
var template = document.createElement('div');
    template.className = 'cv-container';
    template.innerHTML = templateHTML;

// When the body is loaded add the HTML container
var utils = window.optimizely.get('utils');
utils.waitForElement('body').then(function() {
  document.body.appendChild(template);
});

// Form search functionality
var newSearch = document.getElementById('cv-search');
var action = window.location.host + '/us/search';

// Toggles state of the LP
var searchInput = document.querySelector('search__input');
var hero = document.querySelector('.hero');
searchInput.addEventListener('focus', function() {
  this.parentNode.classList.add('active');
  hero.classList.add('search-active');
});

searchInput.addEventListener('blur', function() {
  this.parentNode.classList.remove('active');
  hero.classList.remove('search-active');
});

// Sets the existing search value for hte search suggestions
utils.waitForElement('#searchinput').then(function(el){
  console.log(el);
  searchInput.addEventListener('keyup', function(){
    el.value = this.value;
    console.log(el);
    console.log(el.value);
  });
});

// Makes entire button gender button clickable by forcing click on child anchor el
var genderButtons = document.querySelectorAll('.cv-button');
for (var i = 0; i < genderButtons.length; i++) {
  genderButtons[i].addEventListener('click', function() {
    this.getElementsByTagName('a')[0].click();
  });
}
